          "type": "string",
          "format": "text"
        },
        {
          "name": "icon_url",
          "title": "Avatar",
          "description": "Your bot's Avatar, Default is the one specified in Slack settings",
          "type": "string",
          "format": "image"
        }
      ],
      "deployment_settings": [],
      "resources": [],
      "readme": "readme.md",
      "ui": false,
      "subscriptions": [
        {
          "url": "/notify"
        }
      ]
    }
  },
