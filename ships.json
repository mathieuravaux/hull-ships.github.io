[
  {
    "url": "https://hull-ships.github.io/hull-login/manifest.json",
    "manifest": {
      "name": "Login",
      "description": "A complete user registration and social login modal",
      "picture": "picture.png",
      "readme": "readme.md",
      "index": "ship.js",
      "version": "0.0.1",
      "settings": [
        {
          "name": "logo_image",
          "title": "Logo image",
          "description": "Show this image as your logo on the sign-up and login pages.",
          "type": "string",
          "format": "image"
        },
        {
          "name": "primary_color",
          "title": "Primary color",
          "type": "string",
          "format": "color",
          "default": "#0093E6"
        },
        {
          "name": "text_color",
          "title": "Text color",
          "type": "string",
          "format": "color",
          "default": "#8091AD"
        },
        {
          "name": "link_color",
          "title": "Link color",
          "type": "string",
          "format": "color",
          "default": "#3F4654"
        },
        {
          "name": "background_color",
          "title": "Background color",
          "type": "string",
          "format": "color",
          "default": "#ffffff"
        },
        {
          "name": "overlay_border_radius",
          "title": "Border radius",
          "type": "number",
          "format": "range",
          "default": 6,
          "minimum": 0,
          "maximum": 20
        },
        {
          "name": "show_inline",
          "title": "Show inline",
          "description": "Show login and signup forms in page instead of as a Modal",
          "type": "boolean",
          "default": false
        },
        {
          "name": "show_classic_login",
          "title": "Allow email & password registration",
          "description": "Enable email-based sign ups on your website, i.e. sign ups that don't require social login.",
          "type": "boolean",
          "default": true
        },
        {
          "name": "show_classic_login_as_button",
          "title": "Expand Email form by default",
          "description": "When checked, the email registration form appears inline. Otherwise, it shows a button to expand it",
          "type": "boolean",
          "default": true
        },
        {
          "name": "split_name_field",
          "title": "Split name in two fields",
          "description": "Split the name field in two fields (first name & last name).",
          "type": "boolean",
          "default": false
        },
        {
          "name": "disable_buttons_automatically",
          "title": "Visually disable submit buttons for incomplete forms",
          "description": "If checked, disable the 'log in' and 'sign up' buttons when their forms are incomplete. Incomplete submissions will not be allowed regardless of the state of this option.",
          "type": "boolean",
          "default": true,
          "format": "hidden"
        },
        {
          "name": "show_name_field_on_sign_up",
          "title": "Show name field on sign up",
          "description": "If checked, show a field that asks the user for his name.",
          "type": "boolean",
          "default": true,
          "format": "hidden"
        },
        {
          "name": "show_sign_up_section_after",
          "title": "Automatic Sign up popup delay",
          "description": "This is the wait time (in seconds) before automatically showing the sign-up section to unregistered visitors (once per user). 0 seconds disables the pop-up.",
          "type": "number",
          "format": "number",
          "minimum": 0,
          "default": 0
        },
        {
          "name": "hide_thanks_section_after",
          "title": "Automatically Hide thanks section delay",
          "description": "This is the wait time (in seconds) before automatically hiding the thanks section (i.e. 'thanks for signing up!') once users finish registration (0 to disable).",
          "type": "number",
          "format": "hidden",
          "default": 0
        },
        {
          "name": "show_login",
          "title": "Display log in link",
          "description": "When logged out, show a 'log in' link.",
          "type": "boolean",
          "default": false,
          "format": "hidden"
        },
        {
          "name": "show_signup",
          "title": "Display sign up link",
          "description": "When logged out, show a 'sign up' link.",
          "type": "boolean",
          "default": false,
          "format": "hidden"
        },
        {
          "name": "show_profile",
          "title": "Display user profile link",
          "description": "When logged in, show a link to view and edit their profile.",
          "type": "boolean",
          "default": false,
          "format": "hidden"
        },
        {
          "name": "custom_styles",
          "title": "Custom CSS",
          "description": "Custom CSS to fine-tune Styles.",
          "type": "string",
          "format": "textarea"
        }
      ],
      "deployment_settings": [
        {
          "name": "redirect_url",
          "title": "Redirect URL",
          "description": "Where to redirect when login has succeeded.",
          "type": "string"
        }
      ],
      "resources": [],
      "locales": [
        "en"
      ]
    }
  },
  {
    "url": "https://segment.hull.io/manifest.json",
    "manifest": {
      "name": "Segment",
      "description": "Receive Events from Segment, send updated data to Segment",
      "tags": [
        "incoming",
        "outgoing"
      ],
      "picture": "picture.png",
      "ui": false,
      "admin": "admin.html",
      "readme": "readme.md",
      "version": "0.0.1",
      "deployment_settings": [
        {
          "name": "_selector",
          "default": "body",
          "type": "string",
          "format": "string"
        },
        {
          "name": "_placement",
          "default": "append",
          "type": "string",
          "format": "string"
        }
      ],
      "settings": [
        {
          "name": "write_key",
          "title": "Write Key",
          "description": "Segment Write Key, found in Project Settings > Keys",
          "type": "string",
          "format": "text"
        },
        {
          "name": "handle_groups",
          "title": "Handle Groups",
          "description": "Applies group attributes to as user traits, prefixed with group__",
          "type": "boolean",
          "default": false,
          "format": "hidden"
        }
      ],
      "resources": [],
      "subscriptions": [
        {
          "url": "/notify"
        }
      ]
    }
  },
  {
    "url": "https://hull-optimizely.herokuapp.com/manifest.json",
    "manifest": {
      "name": "Optimizely",
      "description": "Send Hull Segments as Optimizely audiences, use it to personalize for each customer",
      "tags": [
        "client",
        "outgoing"
      ],
      "picture": "picture.png",
      "index": "ship.js",
      "readme": "readme.md",
      "ui": false,
      "version": "0.0.1",
      "settings": [
        {
          "name": "audiences",
          "format": "hidden",
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "audience_id": {
                "type": "integer"
              },
              "segment_id": {
                "type": "string"
              }
            },
            "required": [
              "name",
              "segment_id",
              "audience_id"
            ]
          }
        }
      ],
      "private_settings": [
        {
          "name": "optimizely_api_key",
          "title": "Optimizely API Key",
          "type": "string",
          "required": true
        },
        {
          "name": "optimizely_project_id",
          "title": "Optimizely Project ID",
          "type": "integer",
          "required": true
        }
      ],
      "subscriptions": [
        {
          "url": "/notify"
        }
      ]
    }
  },
  {
    "url": "https://hull-salesforce.herokuapp.com/manifest.json",
    "manifest": {
      "name": "Hull Salesforce",
      "version": "0.1.1",
      "description": "Hull <> Salesforce sync",
      "readme": "readme.md",
      "picture": "picture.png",
      "ui": false,
      "settings": [],
      "private_settings": [
        {
          "name": "salesforce_login",
          "type": "string",
          "required": true
        },
        {
          "name": "salesforce_password",
          "type": "string",
          "required": true
        },
        {
          "name": "salesforce_login_url",
          "type": "string",
          "required": true,
          "default": "https://login.salesforce.com"
        },
        {
          "name": "segment_id",
          "type": "string",
          "title": "Segment ID to sync (optional)",
          "description": "Select a segment to sync with Salesforce",
          "pattern": "^[a-z0-9]{24}$",
          "format": "hidden"
        },
        {
          "name": "leads_mapping",
          "title": "Leads",
          "type": "array",
          "default": [
            {
              "hull_field_name": "first_name",
              "salesforce_field_name": "FirstName",
              "default_value": "[Unknown]",
              "overwrite": false
            },
            {
              "hull_field_name": "last_name",
              "salesforce_field_name": "LastName",
              "default_value": "[Unknown]",
              "overwrite": false
            },
            {
              "hull_field_name": "email",
              "salesforce_field_name": "Email",
              "overwrite": false
            }
          ],
          "items": {
            "type": "object",
            "properties": {
              "hull_field_name": {
                "type": "string",
                "title": "Hull Field"
              },
              "salesforce_field_name": {
                "type": "string",
                "title": "Salesforce Field"
              },
              "overwrite": {
                "type": "boolean",
                "title": "Data from Hull overwrites data on Salesforce",
                "default": false
              },
              "default_value": {
                "type": "string",
                "title": "Default Value"
              },
              "tpl": {
                "type": "string",
                "title": "Template for calculated value"
              }
            },
            "required": [
              "hull_field_name",
              "salesforce_field_name"
            ]
          }
        },
        {
          "name": "contacts_mapping",
          "title": "Contact",
          "type": "array",
          "default": [
            {
              "hull_field_name": "first_name",
              "salesforce_field_name": "FirstName",
              "default_value": "[Unknown]",
              "overwrite": false
            },
            {
              "hull_field_name": "last_name",
              "salesforce_field_name": "LastName",
              "default_value": "[Unknown]",
              "overwrite": false
            },
            {
              "hull_field_name": "email",
              "salesforce_field_name": "Email",
              "overwrite": false
            }
          ],
          "items": {
            "type": "object",
            "properties": {
              "hull_field_name": {
                "type": "string",
                "title": "Hull Field"
              },
              "salesforce_field_name": {
                "type": "string",
                "title": "Salesforce Field"
              },
              "overwrite": {
                "type": "boolean",
                "title": "Data from Hull overwrites data on Salesforce",
                "default": false
              },
              "default_value": {
                "type": "string",
                "title": "Default Value"
              },
              "tpl": {
                "type": "string",
                "title": "Template for calculated value"
              }
            },
            "required": [
              "hull_field_name",
              "salesforce_field_name"
            ]
          }
        }
      ],
      "subscriptions": [
        {
          "url": "/notify"
        }
      ]
    }
  },
  {
    "url": "https://hull-slack.herokuapp.com/manifest.json",
    "manifest": {
      "name": "Slack",
      "description": "Noify a Slack Channel whenever a User enters or leaves a Segment",
      "tags": [
        "outgoing"
      ],
      "picture": "picture.png",
      "version": "0.0.1",
