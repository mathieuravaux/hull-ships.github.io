[
  {
    "url": "https://hull-ships.github.io/hull-quiz/manifest.json",
    "manifest": {
      "name": "quiz",
      "version": "0.0.1",
      "description": "A Wonderful quiz for your users",
      "index": "index.html",
      "fullpage": true,
      "picture": "picture.png",
      "settings": [
        {
          "name": "logo_image",
          "title": "logo image",
          "type": "string",
          "format": "image"
        },
        {
          "name": "background_image",
          "title": "Background image",
          "type": "string",
          "format": "image"
        },
        {
          "name": "background_image_opacity",
          "title": "Background image opacity",
          "type": "string",
          "default": ".8"
        },
        {
          "name": "background_image_blur",
          "title": "Background image blur",
          "type": "string",
          "default": "1"
        },
        {
          "name": "background_color",
          "title": "Background color",
          "type": "string",
          "format": "color",
          "default": "#000000"
        },
        {
          "name": "text_color",
          "title": "text color",
          "type": "string",
          "format": "color",
          "default": "#ffffff"
        },
        {
          "name": "button_background_color",
          "title": "Button background color",
          "type": "string",
          "format": "color",
          "default": "#f39001"
        },
        {
          "name": "button_text_color",
          "title": "Button text color",
          "type": "string",
          "format": "color",
          "default": "#ffffff"
        },
        {
          "name": "display_pagination",
          "title": "Display pagination",
          "description": "Display pagination buttons",
          "type": "boolean",
          "default": false
        },
        {
          "name": "sample_questions",
          "title": "Sample Questions",
          "description": "Only display a number of questions picked randomly",
          "type": "number",
          "format": "number",
          "default": 0
        },
        {
          "name": "sample_answers",
          "title": "Sample answers",
          "description": "Only display a number of answers picked randomly",
          "type": "number",
          "format": "number",
          "default": 0
        },
        {
          "name": "question_countdown",
          "title": "Question countdown",
          "description": "Maximum time to answer a question",
          "type": "number",
          "format": "number",
          "default": 0
        },
        {
          "name": "quiz_countdown",
          "title": "Quiz countdown",
          "description": "Maximum time to answer all questions",
          "type": "number",
          "format": "number",
          "default": 0
        }
      ],
      "resources": [
        {
          "name": "quiz",
          "type": "quiz"
        },
        {
          "name": "form",
          "type": "form"
        }
      ],
      "locales": [
        "en"
      ]
    }
  },
  {
    "url": "https://hull-ships.github.io/hull-ship-boilerplate/manifest.json",
    "manifest": {
      "name": "Hull Ship Boilerplate",
      "description": "Hull Ship Boilerplate",
      "picture": "picture.png",
      "index": "ship.html",
      "version": "0.0.2",
      "locales": [
        "en"
      ],
      "settings": [],
      "resources": []
    }
  },
  {
    "url": "https://hull-ships.github.io/hull-sharing-buttons/manifest.json",
    "manifest": {
      "name": "Hull Sharing button",
      "description": "Hull Sharing button",
      "picture": "picture.png",
      "index": "ship.html",
      "version": "0.0.1",
      "settings": [
        {
          "name": "sharing_buttons",
          "title": "Sharing Buttons",
          "type": "object",
          "properties": {
            "facebook": {
              "title": "Facebook",
              "description": "Show Facebook Button.",
              "type": "boolean",
              "default": true
            },
            "twitter": {
              "title": "Twitter",
              "description": "Show twitter Button.",
              "type": "boolean",
              "default": true
            },
            "google": {
              "title": "Google",
              "description": "Show google Button.",
              "type": "boolean",
              "default": true
            },
            "email": {
              "title": "Email",
              "description": "Show email Button.",
              "type": "boolean",
              "default": true
            },
            "linkedin": {
              "title": "Linkedin",
              "description": "Show linkedin Button.",
              "type": "boolean",
              "default": true
            },
            "whatsapp": {
              "title": "Whatsapp (visible on mobile only)",
              "description": "Show whatsapp Button.",
              "type": "boolean",
              "default": true
            }
          }
        },
        {
          "name": "appearance",
          "title": "Appearance",
          "type": "object",
          "properties": {
            "show_labels": {
              "title": "Show Labels",
              "description": "Show text labels next to icon",
              "type": "boolean",
              "default": true
            },
            "layout": {
              "title": "Layout",
              "description": "",
              "type": "string",
              "enum": [
                "vertical",
                "horizontal"
              ],
              "enum_titles": [
                "Vertical",
                "Horizontal"
              ],
              "default": "horizontal"
            },
            "button_shape": {
              "title": "Button Shape",
              "description": "",
              "type": "string",
              "enum": [
                "circle",
                "rounded",
                "square"
              ],
              "enum_titles": [
                "Circle",
                "Rounded",
                "Square"
              ],
              "default": "square"
            }
          }
        }
      ]
    }
  }
]